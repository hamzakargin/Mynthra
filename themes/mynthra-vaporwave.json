  "tokenColors": [
    {
      "scope": [
        "variable.other.readwrite.destructured",
        "variable.declaration",
        "variable.other",
        "variable.other.constant",
        "variable.other.property.readwrite",
        "entity.name.variable",
        "variable.other.readwrite",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#1AC8FF"
      }
    },
    {
      "scope": [
        "entity.name.class",
        "support.function"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#73DFA5"
      }
    },
    {
      "scope": [
        "meta.jsx.expression",
        "meta.jsx.expression.variable",
        "meta.jsx.children"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": [
        "punctuation.definition.tag",
        "meta.tag",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": [
        "punctuation.separator.comma",
        "punctuation.separator.key-value"
      ],
      "settings": {
        "foreground": "#FF659C"
      }
    },
    {
      "name": "JSON Property Names (Object Keys)",
      "scope": [
        "support.type.property-name.json",
        "meta.structure.dictionary.key.json"
      ],
      "settings": {
        "foreground": "#73DFA5"
      }
    },
    {
      "name": "JavaScript/TypeScript Object Keys",
      "scope": [
        "meta.object-literal.key",
        "string.unquoted"
      ],
      "settings": {
        "foreground": "#1AC8FF"
      }
    },
    {
      "name": "TypeScript/React Property Access",
      "scope": [
        "support.variable.property",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "keyword.control",
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#FF659C"
      }
    },
    {
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#DFD473"
      }
    },
    {
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#E08569"
      }
    },
    {
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#807796"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#7A7FFD"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#DFD473"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#73DFA5"
      }
    },
    {
      "scope": [
        "support.class.component"
      ],
      "settings": {
        "foreground": "#1AC8FF"
      }
    },
    {
      "name": "TypeScript Types & Interfaces",
      "scope": [
        "entity.name.type.interface",
        "entity.name.type.alias",
        "entity.name.type.class",
        "support.type.primitive",
        "support.class.builtin"
      ],
      "settings": {
        "foreground": "#E08569"
      }
    },
    {
      "name": "Generic Type Parameters",
      "scope": [
        "entity.name.type.parameter"
      ],
      "settings": {
        "foreground": "#7A7FFD"
      }
    },
    {
      "name": "Import/Export Keywords",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from"
      ],
      "settings": {
        "foreground": "#FF659C"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "meta.decorator",
        "punctuation.decorator"
      ],
      "settings": {
        "foreground": "#FF659C"
      }
    },
    {
      "name": "Decorator Property Keys",
      "scope": [
        "meta.decorator meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#73DFA5"
      }
    },
    {
      "name": "Class Names in Decorators & Types",
      "scope": [
        "entity.name.type.class",
        "support.class"
      ],
      "settings": {
        "foreground": "#E08569"
      }
    },
    {
      "name": "String Values in Decorators",
      "scope": [
        "meta.decorator string"
      ],
      "settings": {
        "foreground": "#DFD473"
      }
    },
    {
      "name": "Boolean & Null Constants",
      "scope": [
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#7A7FFD"
      }
    }
  ]
}